{"ast":null,"code":"import HelloWorld from './components/HelloWorld.vue';\nimport ElementCommonComp from './components/ElementCommonComp.vue';\nimport { defineComponent, ref } from 'vue';\nimport { ElConfigProvider, ElMessageBox, ElMessage } from 'element-plus';\nexport default defineComponent({\n  components: {\n    ElConfigProvider,\n    HelloWorld,\n    ElementCommonComp\n  },\n  setup() {\n    return {\n      zIndex: 3000,\n      size: 'normal',\n      input: ref(''),\n      dialogVisible: ref(false)\n    };\n  },\n  methods: {\n    clickEventTrigger() {\n      ElMessageBox.alert('ClickEvent', 'Title', {\n        autofocus: false,\n        confirmButtonText: '了解',\n        callback: action => {\n          ElMessage({\n            type: 'info',\n            message: `action: ${action}`\n          });\n        }\n      });\n    }\n  }\n});","map":{"version":3,"names":["HelloWorld","ElementCommonComp","defineComponent","ref","ElConfigProvider","ElMessageBox","ElMessage","components","setup","zIndex","size","input","dialogVisible","methods","clickEventTrigger","alert","autofocus","confirmButtonText","callback","action","type","message"],"sources":["L:\\frontendCore\\vue\\elementUI\\elementuitest\\src\\App.vue"],"sourcesContent":["<template>\n  <el-config-provider :size=\"size\" :z-index=\"zIndex\">\n    <HelloWorld msg=\"Vue.js App With ElementUI-Plus\"/>\n    <el-row class=\"mb-4\">\n      <el-col :span=\"24\">\n        <el-space wrap>\n          <el-button type=\"primary\" @click=\"clickEventTrigger\">Default</el-button>\n          <el-button type=\"primary\" @click=\"clickEventTrigger\" plain>Plain</el-button>\n          <el-button type=\"primary\" @click=\"clickEventTrigger\" round>Round</el-button>\n          <el-input v-model=\"input\" class=\"w-50 m-2\" placeholder=\"Please input\" clearable />\n          <el-tag>Tag 1</el-tag>\n          <el-tag class=\"ml-2\" type=\"success\">Tag 2</el-tag>\n          <el-tag class=\"ml-2\" type=\"info\">Tag 3</el-tag>\n          <el-tag class=\"ml-2\" type=\"warning\">Tag 4</el-tag>\n          <el-tag class=\"ml-2\" type=\"danger\">Tag 5</el-tag>\n        </el-space>\n      </el-col>\n    </el-row>\n\n    <el-divider />\n\n    <el-row class=\"mb-4\">\n      <el-col :span=\"24\">\n        <el-space wrap>\n          <ElementCommonComp />\n        </el-space>\n      </el-col>\n    </el-row>\n\n    <el-dialog\n      v-model=\"dialogVisible\"\n      title=\"测试\"\n      width=\"300px\"\n    >\n      <span>Click事件</span>\n      <template #footer>\n        <span class=\"dialog-footer\">\n          <el-button @click=\"dialogVisible = false\">Cancel</el-button>\n          <el-button type=\"primary\" @click=\"dialogVisible = false\">\n            Confirm\n          </el-button>\n        </span>\n      </template>\n    </el-dialog>\n  </el-config-provider>\n</template>\n\n<script>\nimport HelloWorld from './components/HelloWorld.vue'\nimport ElementCommonComp from './components/ElementCommonComp.vue'\nimport { defineComponent, ref } from 'vue';\nimport {ElConfigProvider, ElMessageBox, ElMessage} from 'element-plus'\n\nexport default defineComponent ({\n  components: {\n    ElConfigProvider,\n    HelloWorld,\n    ElementCommonComp\n  },\n  setup() {\n    return {\n      zIndex: 3000,\n      size: 'normal',\n      input: ref(''),\n      dialogVisible: ref(false)\n    }\n  },\n  methods: {\n    clickEventTrigger(){\n      ElMessageBox.alert('ClickEvent', 'Title', {\n        autofocus: false,\n        confirmButtonText: '了解',\n        callback: action => {\n          ElMessage({\n            type: 'info',\n            message: `action: ${action}`\n          })\n        }\n      })\n    }\n  },\n})\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n</style>\n"],"mappings":"AAgDA,OAAOA,UAAS,MAAO,6BAA4B;AACnD,OAAOC,iBAAgB,MAAO